    "@ngrx/store": "^7.0.0",
    "core-js": "^2.6.2",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/cli": "^7.2.2",
    "@angular/compiler-cli": "^7.2.1",
